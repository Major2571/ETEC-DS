programa
{
	
	funcao inicio()
	{
		real C, F
		
		escreva (" Digite a temperatura (°C): ")
		leia (C)

		F = ( 9 * C + 160) / 5

		escreva (" Sua temperatura em Fahrenheit é igual a: ", F, " °F")
	}
}

